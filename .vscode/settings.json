{
  "peacock.color": "#0f74f7",
  "editor.defaultFormatter": "rvest.vs-code-prettier-eslint",
  "editor.formatOnPaste": false, // required
  "editor.formatOnType": false, // required
  "editor.formatOnSave": true, // optional
  "editor.formatOnSaveMode": "file", // required to format on save
  "files.autoSave": "onFocusChange", // optional but recommended
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#4091f9",
    "activityBar.background": "#4091f9",
    "activityBar.foreground": "#15202b",
    "activityBar.inactiveForeground": "#15202b99",
    "activityBarBadge.background": "#b90654",
    "activityBarBadge.foreground": "#e7e7e7",
    "commandCenter.border": "#e7e7e799",
    "sash.hoverBorder": "#4091f9",
    "statusBar.background": "#0f74f7",
    "statusBar.foreground": "#e7e7e7",
    "statusBarItem.hoverBackground": "#4091f9",
    "statusBarItem.remoteBackground": "#0f74f7",
    "statusBarItem.remoteForeground": "#e7e7e7",
    "titleBar.activeBackground": "#0f74f7",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveBackground": "#0f74f799",
    "titleBar.inactiveForeground": "#e7e7e799"
  },
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/.classpath": true,
    "**/.factorypath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/*.cjs": true,
    "**/*.class": true,
    "**/*dump*.sql": true,
    "**/build/": true,
    "**/WWW/*": true,
    "node_modules/": true
  },
  "explorerExclude.backup": {}
}
