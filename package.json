{
  "name": "agile-weather",
  "version": "0.0.1",
  "description": "Sample project to explore Redwood tech stack",
  "license": "UNLICENSED",
  "private": true,
  "workspaces": {
    "packages": [
      "api",
      "web",
      "packages/*"
    ]
  },
  "devDependencies": {
    "@redwoodjs/core": "^3.7.1",
    "cspell": "^6.12.x"
  },
  "eslintConfig": {
    "extends": "@redwoodjs/eslint-config",
    "root": true
  },
  "engines": {
    "node": "16.x",
    "yarn": "3.x"
  },
  "prisma": {
    "seed": "yarn rw exec seed"
  },
  "scripts": {
    "test:spelling": "cspell --relative \"*.md\" \"{api,web,scripts}/**/*.{md,js,ts,tsx}\""
  },
  "packageManager": "yarn@3.2.3",
  "dependencies": {
    "@prisma/client": "^4.4.0",
    "chart.js": "^3.9.1",
    "cuid": "^2.1.8",
    "fast-csv": "^4.3.6",
    "radash": "^7.1.0",
    "react-chartjs-2": "^4.3.1"
  }
}
